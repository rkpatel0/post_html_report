<script type="text/javascript">
var navpad_static = 1;
var navpad = 0;
if (typeof(MaxFire_Navpad_Manager) == "function") {
    navpad = new MaxFire_Navpad_Manager();
}
if (navpad && navpad_static) {
    navpad.prep_slides();
}

function open_navpad(sname)
{
    // If there is no navpad, just use the plain navbar.
    if (! navpad) { return (click_navbar(sname)); }
    if (! snp_active) { return (click_navbar(sname)); }

    // Disable the navpad for certain older browsers.
    if (Agent && Agent.is_msie6_relic()) { return (click_navbar(sname)); }

    // If the navpad is already initd, just switch to the tab.
    if (navpad.good2go) { navpad.click_tab(sname); return (false); }

    // Init various navpad values.
    navpad.sname = sname;
    navpad.section = 'base.mvp';

    // Load the navpad content.
    navpad.load_base("prep_navpad");
}
function prep_navpad(wrapper)
{
    // If no data provided on callback, punt.
    if (! wrapper) { return (click_navbar()); }
    if (! wrapper.mdata) { return (click_navbar()); }
    if (! wrapper.success) { return (click_navbar()); }

    // If the target element is not there, punt.
    var router_elem = navpad.router_elem;
    if (! router_elem) { return (click_navbar()); }

    // Put the dynamic navpad content in place.
    router_elem.innerHTML = wrapper.mdata.base_content;

    // Now that we have content, prep the navpad.
    navpad.prep_slides();
    navpad.click_tab(navpad.sname);
    navpad.secmap[navpad.section] = 1;

    // Make sure it worked.
    var looks_good = 1; // TBD (bgcolor non-white)
    if (! looks_good) { return (click_navbar()); }
    return (true);
}

function view_section(sname, stitle, file_name)
{
    // If the section is already initd, take the easy way out.
    /*-----
    NOTE: Ajax.url contains the full blown queried url.
    if (navpad.section[file_name]) {
	// snp-router-s1p1
	// snp-router-s1p2 <stuff>
	var slide_elem = navpad.slide_elem(navpad.sname + 'p2');
    }
    -----*/

    // Load the navpad content.
    //if (Mary[1] != 1) { return (false); }
    navpad.load_section("view_section_handler", file_name, { });
    navpad.stitle = stitle;
}
function view_section_handler(wrapper)
{
    // If no data provided on callback, punt.
    if (! wrapper) { return (click_error("no wrapper")); }
    if (! wrapper.mdata) { return (click_error("no mdata")); }
    if (! wrapper.success) { return (click_error("no success")); }

    // Get the current slide element panel.
    var eid = "snp-" + navpad.stitle + "-body";
    var elem = dyna.elem(eid);
    if (! elem) { return (click_error("no body elem: " + eid)); }

    // Put the dynamic navpad content in place.
    elem.innerHTML = wrapper.mdata.slide_content;

    // Answer back.
    return (true);
}

function click_navbar(sname)
{
    if (! sname) { sname = navpad.sname; }
    var eid = 'snb-' + sname + '-link';
    var link = document.getElementById(eid);
    if (! link) { alert("ERROR: link not found: " + eid); return (false); }
    if (! link.href) { alert("ERROR: bad link: " + eid); return (false); }
    window.location = link.href;
    return (false);
}
function click_error(msg)
{
    alert("ERROR: " + msg);
    return (false);
}
</script>

